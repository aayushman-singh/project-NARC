{
  "name": "project-narc",
  "version": "1.0.0",
  "description": "Monorepo for Project NARC containing scraper, frontend, and backend",
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.693.0",
    "aws": "^0.0.3-2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongodb": "^6.10.0",
    "mongoose": "^8.8.1",
    "puppeteer": "^23.7.1",
    "rollup": "^4.21.2",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.9.3",
    "@types/pg": "^8.11.8",
    "concurrently": "^8.2.2",
    "lerna": "^8.1.8",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
  },
  "private": true,
  "workspaces": [
    "scraper",
    "frontend",
    "backend"
  ],
  "scripts": {
    "start:mobile": "cd mobileApp && flutter run",
    "start:script": "python frontend/script/server.py",
    "start:telegram": "python scraper/src/routes/telegram.py",
    "start:auth": "npx tsx backend/server",
    "start:maigret": "py frontend/maigret/server.py",
    "start:instagram": "cd scraper/src/routes/ && npx tsx src/routes/instagram.ts",
    "start:x": "cd scraper/src/routes/ && npx tsx src/routes/x.ts",
    "start:facebook": "cd scraper/src/routes/ && npx tsx src/routes/facebook.ts",
    "start:whatsapp": "npx tsx scraper/src/routes/whatsapp.ts",
    "start:frontend": "cd frontend && npm run dev",
    "start:all": "npm-run-all --parallel start:maigret start:instagram start:x start:facebook start:frontend start:auth start:script start:telegram"
  }
}
