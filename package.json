{
  "name": "project-narc",
  "version": "1.0.0",
  "description": "Monorepo for Project NARC containing scraper, frontend, and backend",
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.693.0",
    "aws": "^0.0.3-2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "googleapis": "^144.0.0",
    "mongodb": "^6.10.0",
    "mongoose": "^8.8.1",
    "puppeteer": "^23.7.1",
    "react-oauth": "^0.0.1",
    "react-social-icons": "^6.18.0",
    "rollup": "^4.21.2",
    "uuid": "^11.0.3",
    "webdriverio": "^9.4.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.8",
    "concurrently": "^8.2.2",
    "lerna": "^8.1.8",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.0",
    "typescript": "^5.7.2"
  },
  "private": true,
  "workspaces": [
    "scraper",
    "frontend",
    "backend"
  ],
  "scripts": {
    "start:timeline": "npx tsx  scraper/src/routes/timeline.ts",
    "start:discord": "npx tsx  scraper/src/routes/discord.ts",
    "start:gdrive": "npx tsx  scraper/src/routes/gdrive.ts",
    "start:google": "npx tsx  scraper/src/routes/google.ts",
    "start:youtube": "npx tsx  scraper/src/routes/youtube.ts",
    "start:mobile": "cd mobileApp && flutter run",
    "start:gmail": "npx tsx scraper/src/routes/gmail.ts",
    "start:drive": "npx tsx scraper/src/routes/gdrive.ts",
    "start:telegram": "python scraper/src/routes/telegram.py",
    "start:auth": "npx tsx backend/server",
    "start:maigret": "python frontend/maigret/server.py",
    "start:instagram": "cd scraper/src/routes/ && npx tsx src/routes/instagram.ts",
    "start:x": "cd scraper/src/routes/ && npx tsx src/routes/x.ts",
    "start:facebook": "cd scraper/src/routes/ && npx tsx src/routes/facebook.ts",
    "start:whatsapp": "npx tsx scraper/src/routes/whatsapp.ts",
    "start:frontend": "cd frontend && npm run dev",
    "start:all": "npm-run-all --parallel start:maigret start:instagram start:x start:facebook start:frontend start:auth start:google start:gdrive start:youtube start:telegram start:discord start:timeline"
  }
}
